<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/"
     xml:base="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/"
     xmlns:dbpedia="http://dbpedia.org/ontology/">
    <owl:Ontology rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1">
        <terms:creator xml:lang="it">Nicolò Maria Polizzi</terms:creator>
        <terms:date>2024-12-06</terms:date>
        <terms:license>https://creativecommons.org/licenses/by/4.0/</terms:license>
        <terms:title xml:lang="en">Monster Ontology</terms:title>
        <rdfs:comment xml:lang="it">Ontologia per la rappresentazione di mostri cinematografici, i loro poteri, origini, battaglie e i film in cui appaiono.</rdfs:comment>
        <rdfs:label xml:lang="en">Monster Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/date"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/appearsIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/appearsIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <rdfs:comment xml:lang="it">Indica il film in cui una creatura appare</rdfs:comment>
        <rdfs:label xml:lang="en">appearsIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/belongsToSaga -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/belongsToSaga">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/appearsIn"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/isSequel"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Indica che una creatura fa parte di un Franchise</rdfs:comment>
        <rdfs:label xml:lang="en">belongsToFranchise</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/directedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/directedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Director"/>
        <rdfs:comment xml:lang="it">Indica il regista che ha diretto un determinato film</rdfs:comment>
        <rdfs:label xml:lang="en">directedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/fights -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/fights">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/foughtBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Indica che una creatura combatte contro un&apos;altra creatura</rdfs:comment>
        <rdfs:label xml:lang="en">fights</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/foughtBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/foughtBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Proprietà inversa di fights, indica che una creatura è combattuta da un&apos;altra</rdfs:comment>
        <rdfs:label xml:lang="en">foughtBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/happensIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/happensIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <rdfs:comment xml:lang="it">Indica il film in cui si svolge una determinata battaglia</rdfs:comment>
        <rdfs:label xml:lang="en">happensIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasOrigin -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasOrigin">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Origin"/>
        <rdfs:comment xml:lang="it">Indica l&apos;origine di una creatura (nucleare, morso, infezione)</rdfs:comment>
        <rdfs:label xml:lang="en">hasOrigin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasParticipant -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasParticipant">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/participatesIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Indica le creature che partecipano a una determinata battaglia</rdfs:comment>
        <rdfs:label xml:lang="en">hasParticipant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasPower -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasPower">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Power"/>
        <rdfs:comment xml:lang="it">Indica un potere o abilità posseduta da una creatura</rdfs:comment>
        <rdfs:label xml:lang="en">hasPower</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasWinner -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasWinner">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Indica il vincitore di una battaglia</rdfs:comment>
        <rdfs:label xml:lang="en">hasWinner</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/isSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/isSequel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <rdfs:comment xml:lang="it">Indica che un film è il sequel di un altro film. Proprietà transitiva</rdfs:comment>
        <rdfs:label xml:lang="en">isSequel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/participatesIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/participatesIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle"/>
        <rdfs:comment xml:lang="it">Inverso di hasParticipant, indica che una creatura fa parte di una battaglia</rdfs:comment>
        <rdfs:label xml:lang="it">participatesIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/sharesOriginWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/sharesOriginWith">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Due creature condividono la stessa origine</rdfs:comment>
        <rdfs:label xml:lang="en">sharesOriginWith</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/firstMovie -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/firstMovie">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Primo film in cui quella creatura appare</rdfs:comment>
        <rdfs:label xml:lang="en">firstMovie</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/height -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/height">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="it">Altezza della creatura</rdfs:comment>
        <rdfs:label xml:lang="en">height</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/powerLevel -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/powerLevel">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="it">Livello numerico che indica la potenza di una creatura</rdfs:comment>
        <rdfs:label xml:lang="en">powerLevel</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/releaseYear -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/releaseYear">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="it">Anno di uscita di un film</rdfs:comment>
        <rdfs:label xml:lang="en">releaseDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/FictionalCharacter -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/FictionalCharacter">
        <rdfs:comment xml:lang="en">Ontologia esterna di creature di finzione</rdfs:comment>
        <rdfs:label xml:lang="en">dbo:FictionalCharacters</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Movie -->

    <owl:Class rdf:about="http://schema.org/Movie">
        <rdfs:comment xml:lang="it">Ontologia esterna di film</rdfs:comment>
        <rdfs:label xml:lang="en">schema:Movie</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Person -->

    <owl:Class rdf:about="http://schema.org/Person">
        <rdfs:comment xml:lang="it">Ontologia esterna di persone</rdfs:comment>
        <rdfs:label xml:lang="en">schema:Person</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/AggressiveCreature -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/AggressiveCreature">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/participatesIn"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByRole"/>
        <rdfs:comment xml:lang="it">Una creatura che ha partecipato ad almeno una battaglia con un&apos;altra, viene definita aggressiva</rdfs:comment>
        <rdfs:label xml:lang="en">Aggressive Creature</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Antagonist -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Antagonist">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByRole"/>
        <rdfs:comment xml:lang="it">Antagonista di un film, la creatura che perde dal protagonista</rdfs:comment>
        <rdfs:label xml:lang="en">Antagonist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle">
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Franchise"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Origin"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Power"/>
        <rdfs:comment xml:lang="it">Evento per gestire le battaglie tra due creature</rdfs:comment>
        <rdfs:label xml:lang="en">Battle</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature">
        <rdfs:subClassOf rdf:resource="http://dbpedia.org/ontology/FictionalCharacter"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Origin"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Power"/>
        <rdfs:comment xml:lang="it">Rappresenta un mostro o creatura cinematografica che appare in uno o più film</rdfs:comment>
        <rdfs:label xml:lang="en">Creature</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByOrigin -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByOrigin">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Classificazione delle creature in base alla loro origine</rdfs:comment>
        <rdfs:label xml:lang="en">Creature By Origin</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByRole -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByRole">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature"/>
        <rdfs:comment xml:lang="it">Categorizza le creature in base alla loro natura</rdfs:comment>
        <rdfs:label xml:lang="en">Creature By Role</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Crew -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Crew">
        <rdfs:subClassOf rdf:resource="http://schema.org/Person"/>
        <rdfs:comment xml:lang="it">Persone che hanno lavorato ad un Film</rdfs:comment>
        <rdfs:label xml:lang="en">Film Crew</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Director -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Director">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Crew"/>
        <rdfs:comment xml:lang="it">Regista cinematografico che ha diretto uno o più film</rdfs:comment>
        <rdfs:label xml:lang="en">Director</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/DominantCreature -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/DominantCreature">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByRole"/>
        <rdfs:comment xml:lang="it">Creatura che vince più battaglie</rdfs:comment>
        <rdfs:label xml:lang="en">Dominant Creature</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Franchise -->

    <owl:Class rdf:about="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Franchise">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/appearsIn"/>
