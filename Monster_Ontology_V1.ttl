@prefix : <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/> .

<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1> rdf:type owl:Ontology ;
                                                                           rdfs:comment """Ontologia per la rappresentazione di mostri cinematografici, i loro poteri, origini, battaglie e i film in cui appaiono. 
Autore: Nicolò Maria Polizzi. 
Data: 2024-12-06. 
Prefix: monster""" ;
                                                                           rdfs:label "Monster Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#hasWinner
:hasWinner rdf:type owl:ObjectProperty ;
           rdfs:comment "Indica il vincitore di una battaglia"@it ;
           rdfs:label "hasWinner"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasParticipant ;
                rdfs:comment "Inverso di hasParticipant, indica che una creatura fa parte di una battaglia"@it ;
                rdfs:label "participatesIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/appearsIn
:appearsIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Creature ;
           rdfs:range :Film ;
           rdfs:comment "Indica il film in cui una creatura appare"@it ;
           rdfs:label "appearsIn"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/belongsToSaga
:belongsToSaga rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Creature ;
               rdfs:range :Film ;
               owl:propertyChainAxiom ( :appearsIn
                                        :isSequel
                                      ) ;
               rdfs:comment "Indica che una creatura fa parte di un Franchise"@en ;
               rdfs:label "belongsToFranchise"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/directedBy
:directedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Film ;
            rdfs:range :Director ;
            rdfs:comment "Indica il regista che ha diretto un determinato film"@it ;
            rdfs:label "directedBy"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/fights
:fights rdf:type owl:ObjectProperty ;
        owl:inverseOf :foughtBy ;
        rdfs:domain :Creature ;
        rdfs:range :Creature ;
        rdfs:comment "Indica che una creatura combatte contro un'altra creatura"@it ;
        rdfs:label "fights"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/foughtBy
:foughtBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Creature ;
          rdfs:range :Creature ;
          rdfs:comment "Proprietà inversa di fights, indica che una creatura è combattuta da un'altra"@it ;
          rdfs:label "foughtBy"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/happensIn
:happensIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Battle ;
           rdfs:range :Film ;
           rdfs:comment "Indica il film in cui si svolge una determinata battaglia" ;
           rdfs:label "happensIn"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Creature ;
           rdfs:range :Origin ;
           rdfs:comment "Indica l'origine di una creatura (nucleare, morso, infezione)"@it ;
           rdfs:label "hasOrigin"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                rdfs:domain :Battle ;
                rdfs:range :Creature ;
                rdfs:comment "Indica le creature che partecipano a una determinata battaglia"@it ;
                rdfs:label "hasParticipant"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/hasPower
:hasPower rdf:type owl:ObjectProperty ;
          rdfs:domain :Creature ;
          rdfs:range :Power ;
          rdfs:comment "Indica un potere o abilità posseduta da una creatura"@it ;
          rdfs:label "hasPower"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/isSequel
:isSequel rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain :Film ;
          rdfs:range :Film ;
          rdfs:comment "Indica che un film è il sequel di un altro film. Proprietà transitiva"@it ;
          rdfs:label "isSequel"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/powerLevel
:powerLevel rdf:type owl:DatatypeProperty ;
            rdfs:domain :Creature ;
            rdfs:range xsd:integer ;
            rdfs:comment "Livello numerico che indica la potenza di una creatura"@it ;
            rdfs:label "powerLevel"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/releaseYear
:releaseYear rdf:type owl:DatatypeProperty ;
             rdfs:domain :Film ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "Anno di uscita di un film"@it ;
             rdfs:label "releaseYear"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#AggressiveCreature
:AggressiveCreature rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :participatesIn ;
                                          owl:someValuesFrom :Battle
                                        ] ;
                    rdfs:subClassOf :CreatureType ;
                    rdfs:comment "Una creatura che ha partecipato ad almeno una battaglia con un'altra, viene definita aggressiva"@it ;
                    rdfs:label "Aggressive Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Creature_Characteristics
:Creature_Characteristics rdf:type owl:Class ;
                          rdfs:subClassOf :Creature ;
                          rdfs:comment "Classificazione delle creature in base ai loro valori di potere e le loro debolezze"@it ;
                          rdfs:label "Creature Characteristics"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#HighlyDangerousCreature
:HighlyDangerousCreature rdf:type owl:Class ;
                         rdfs:subClassOf :CreatureType ;
                         rdfs:comment "Una creatura con un power level eccessivo"@it ;
                         rdfs:label "Highly Dangerous Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle
:Battle rdf:type owl:Class .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature
:Creature rdf:type owl:Class ;
          rdfs:comment "Rappresenta un mostro o creatura cinematografica che appare in uno o più film"@it ;
          rdfs:label "Creature"@en ;
          rdfs:seeAlso <http://dbpedia.org/ontology/FictionalCharacter> .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureType
:CreatureType rdf:type owl:Class ;
              rdfs:subClassOf :Creature ;
              rdfs:comment "Classificazione delle creature in base alla loro origine o natura"@it ;
              rdfs:label "Creature Type"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Director
:Director rdf:type owl:Class ;
          rdfs:subClassOf :FilmCrew ;
          rdfs:comment "Regista cinematografico che ha diretto uno o più film" ;
          rdfs:label "Director"@en ;
          rdfs:seeAlso <http://schema.org/Person> .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Film
:Film rdf:type owl:Class ;
      rdfs:seeAlso <http://schema.org/Movie> .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/FilmCrew
:FilmCrew rdf:type owl:Class ;
          rdfs:subClassOf :Film ;
          rdfs:comment "Persone che hanno lavorato ad un Film"@it ;
          rdfs:label "Film Crew"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Franchise
:Franchise rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :appearsIn ;
                                 owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Film
                               ] ;
           rdfs:subClassOf :Film ;
           rdfs:comment "Serie di film collegati tra loro, definita come un insieme di almeno due film correlati"@it ;
           rdfs:label "Franchise"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/NuclearCreature
:NuclearCreature rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasOrigin ;
                                       owl:hasValue :Nuclear
                                     ] ;
                 rdfs:subClassOf :CreatureType ;
                 rdfs:comment "Creatura la cui origine è legata all'energia nucleare o alle radiazioni"@it ;
                 rdfs:label "Nuclear Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Origin
:Origin rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Bite
                                          :Infection
                                          :Nuclear
                                        )
                            ] ;
        rdfs:comment "Classe enumerata che rappresenta le possibili origini di una creatura: morso, infezione o nucleare" ;
        rdfs:label "Origin"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Power
:Power rdf:type owl:Class ;
       rdfs:subClassOf :Creature_Characteristics ;
       rdfs:comment "Potere o abilità speciale posseduta da una creatura"@it ;
       rdfs:label "Power"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/VampireCreature
:VampireCreature rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasOrigin ;
                                       owl:hasValue :Bite
                                     ] ;
                 rdfs:subClassOf :CreatureType ;
                 rdfs:comment "Creatura la cui origine è legata al morso di un vampiro" ;
                 rdfs:label "Vampire Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Weakness
:Weakness rdf:type owl:Class ;
          rdfs:subClassOf :Creature_Characteristics ;
          rdfs:comment "Debolezza o vulnerabilità di una creatura"@it ;
          rdfs:label "Weakness"@en .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Adam_Wingard
:Adam_Wingard rdf:type owl:NamedIndividual ;
              rdfs:comment "Regista americano noto per Godzilla vs. Kong"@it ;
              rdfs:label "Adam Wingard"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle1
:Battle1 rdf:type owl:NamedIndividual ;
         :hasWinner :Godzilla ;
         :happensIn :Ghidorah_The_ThreeHeaded_Monster ;
         :hasParticipant :Godzilla ,
                         :King_Ghidora ;
         rdfs:comment "Battaglia tra Godzilla e King Ghidorah avvenuta nel film Ghidorah, the Three-Headed Monster"@it ;
         rdfs:label "Battle of Godzilla vs King Ghidorah (1964)"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Bite
:Bite rdf:type owl:NamedIndividual ;
      rdfs:comment "Origine legata al morso di una creatura soprannaturale come un vampiro"@it ;
      rdfs:label "Bite"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Dracula
:Dracula rdf:type owl:NamedIndividual ;
         :appearsIn :Dracula_1931 ;
         :hasOrigin :Bite ;
         :hasPower :Regeneration ;
         :powerLevel 500 ;
         rdfs:comment "Vampiro leggendario originato da un morso, protagonista dell'omonimo film del 1931. Possiede il potere della rigenerazione"@it ;
         rdfs:label "Dracula"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Dracula_1931
:Dracula_1931 rdf:type owl:NamedIndividual ;
              :directedBy :Tod_Browning ;
              :releaseYear "1964-02-14T00:00:00"^^xsd:dateTime ;
              rdfs:comment "Film horror classico del 1931 diretto da Tod Browning, con Bela Lugosi nel ruolo di Dracula"@it ;
              rdfs:label "Dracula (1931)"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Firebreath
:Firebreath rdf:type owl:NamedIndividual ;
            rdfs:comment "Soffio atomico, potere caratteristico di Godzilla e altre creature nucleari"@it ;
            rdfs:label "Atomic Breath"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Ghidorah_The_ThreeHeaded_Monster
:Ghidorah_The_ThreeHeaded_Monster rdf:type owl:NamedIndividual ;
                                  owl:differentFrom :Godzilla_1954 ;
                                  :directedBy :Ishiro_Honda ;
                                  :isSequel :Godzilla_1954 ;
                                  :releaseYear "1964-12-20T00:00:00"^^xsd:dateTime ;
                                  rdfs:comment "Film del 1964 diretto da Ishiro Honda, sequel di Godzilla 1954, in cui appare per la prima volta King Ghidorah"@it ;
                                  rdfs:label "Ghidorah, the Three-Headed Monster"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Godzilla
:Godzilla rdf:type owl:NamedIndividual ;
          owl:differentFrom :King_Ghidora ;
          :appearsIn :Ghidorah_The_ThreeHeaded_Monster ,
                     :Godzilla_1954 ;
          :hasOrigin :Nuclear ;
          :hasPower :Firebreath ;
          :powerLevel 800 ;
          rdfs:comment "Mostro gigante di origine nucleare, protagonista dell'omonimo franchise giapponese. Possiede il potere del soffio atomico"@it ;
          rdfs:label "Godzilla"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Godzilla_1954
:Godzilla_1954 rdf:type owl:NamedIndividual ;
               :directedBy :Yoshimitsu_Banno ;
               :releaseYear "1954-05-14T00:00:00"^^xsd:dateTime ;
               rdfs:comment "Primo film del franchise Godzilla, diretto da Yoshimitsu Banno e uscito nel 1954"@it ;
               rdfs:label "Godzilla 1954"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Godzilla_vs_Kong
:Godzilla_vs_Kong rdf:type owl:NamedIndividual ;
                  :directedBy :Adam_Wingard ;
                  :isSequel :Ghidorah_The_ThreeHeaded_Monster ;
                  :releaseYear "2021-03-31T00:00:00"^^xsd:dateTime ;
                  rdfs:comment "Film del 2021 diretto da Adam Wingard, parte del MonsterVerse"@it ;
                  rdfs:label "Godzilla vs. Kong"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Infection
:Infection rdf:type owl:NamedIndividual ;
           rdfs:comment "Origine legata a un'infezione virale o batterica"@it ;
           rdfs:label "Infection"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Ishiro_Honda
:Ishiro_Honda rdf:type owl:NamedIndividual ;
              rdfs:comment "Regista giapponese noto per aver diretto numerosi film di Godzilla"@it ;
              rdfs:label "Ishiro Honda"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/King_Ghidora
:King_Ghidora rdf:type owl:NamedIndividual ;
              :appearsIn :Ghidorah_The_ThreeHeaded_Monster ;
              :hasOrigin :Nuclear ;
              :hasPower :Firebreath ;
              :powerLevel 900 ;
              rdfs:comment "Drago a tre teste di origine nucleare, principale antagonista di Godzilla"@it ;
              rdfs:label "King Ghidora"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Nuclear
:Nuclear rdf:type owl:NamedIndividual ;
         rdfs:comment "Origine legata all'energia nucleare o alle radiazioni atomiche"@it ;
         rdfs:label "Nuclear"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Regeneration
:Regeneration rdf:type owl:NamedIndividual ;
              rdfs:comment "Capacità di rigenerare parti del corpo danneggiate"@it ;
              rdfs:label "Regeneration"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Tod_Browning
:Tod_Browning rdf:type owl:NamedIndividual ;
              rdfs:comment "Regista americano noto per il film Dracula del 1931"@it ;
              rdfs:label "Tod Browning"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Yoshimitsu_Banno
:Yoshimitsu_Banno rdf:type owl:NamedIndividual ;
                  rdfs:comment "Regista giapponese che ha diretto film del franchise Godzilla"@it ;
                  rdfs:label "Yoshimitsu Banno"@en .


#################################################################
#    Rules
#################################################################

:b rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:m1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:m2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:c rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:lvl rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Indica che se in una battaglia vi sono due partecipanti distinti, allora essi combattono" ;
   rdfs:label "BattleParticipantsFight" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> :Battle ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :b
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#propertyPredicate> :hasParticipant ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> :b ;
                                                                  <http://www.w3.org/2003/11/swrl#argument2> :m1
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#propertyPredicate> :hasParticipant ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> :b ;
                                                                             <http://www.w3.org/2003/11/swrl#argument2> :m2
                                                                           ] ;
                                                                 rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                            rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DifferentIndividualsAtom> ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument1> :m1 ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument2> :m2
                                                                                      ] ;
                                                                            rdf:rest rdf:nil
                                                                          ]
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :fights ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :m1 ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :m2
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Indica che se una creatura ha un powerLevel troppo elevato diventa molto pericolosa" ;
   rdfs:label "Highly Dangerous Creatures" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> :Creature ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :c
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#propertyPredicate> :powerLevel ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> :c ;
                                                                  <http://www.w3.org/2003/11/swrl#argument2> :lvl
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#BuiltinAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#builtin> <http://www.w3.org/2003/11/swrlb#greaterThan> ;
                                                                             <http://www.w3.org/2003/11/swrl#arguments> [ rdf:type rdf:List ;
                                                                                                                          rdf:first :lvl ;
                                                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                                                     rdf:first 800 ;
                                                                                                                                     rdf:rest rdf:nil
                                                                                                                                   ]
                                                                                                                        ]
                                                                           ] ;
                                                                 rdf:rest rdf:nil
                                                               ]
