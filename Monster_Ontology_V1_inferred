@prefix : <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/> .

<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/> rdf:type owl:Ontology ;
                                                                            <http://purl.org/dc/terms/creator> "Nicolò Maria Polizzi"@it ;
                                                                            <http://purl.org/dc/terms/date> "2024-12-06" ;
                                                                            <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/" ;
                                                                            <http://purl.org/dc/terms/title> "Monster Ontology"@en ;
                                                                            <http://purl.org/vocab/vann/preferredNamespacePrefix> "mo" ;
                                                                            <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/" ;
                                                                            rdfs:comment "Ontologia per la rappresentazione di mostri cinematografici, i loro poteri, origini, battaglie e i film in cui appaiono."@it ;
                                                                            rdfs:label "Monster Ontology"@en .

