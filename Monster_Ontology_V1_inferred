###  http://dbpedia.org/ontology/FictionalCharacter
<http://dbpedia.org/ontology/FictionalCharacter> rdf:type owl:Class ;
                                                 rdfs:comment "Ontologia esterna di creature di finzione"@en ;
                                                 rdfs:label "dbo:FictionalCharacters"@en .


###  http://schema.org/Movie
<http://schema.org/Movie> rdf:type owl:Class ;
                          rdfs:comment "Ontologia esterna di film"@it ;
                          rdfs:label "schema:Movie"@en .


###  http://schema.org/Person
<http://schema.org/Person> rdf:type owl:Class ;
                           rdfs:comment "Ontologia esterna di persone"@it ;
                           rdfs:label "schema:Person"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Alignment
<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Alignment> rdf:type owl:Class ;
                                                                                    rdfs:comment "Indica se una creatura è considerata buona, cattiva o se è un antieroe"@it ;
                                                                                    rdfs:label "Alignment"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Antagonist
<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Antagonist> rdf:type owl:Class ;
                                                                                     rdfs:subClassOf :CreatureByRole ;
                                                                                     owl:disjointWith :MonsterMovies ,
                                                                                                      :Origin ,
                                                                                                      :Power ;
                                                                                     rdfs:comment "Creatura che combatte contro un buono o un antieroe"@it ;
                                                                                     rdfs:label "Antagonist"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#EvilAlignment
<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#EvilAlignment> rdf:type owl:Class ;
                                                                                        rdfs:subClassOf <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Alignment> ;
                                                                                        rdfs:comment "Creatura che combatte con ideologie distruttive"@it ;
                                                                                        rdfs:label "Evil Alignment"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#GoodAlignment
<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#GoodAlignment> rdf:type owl:Class ;
                                                                                        rdfs:subClassOf <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Alignment> ;
                                                                                        rdfs:comment "Una creatura che combatte per il bene"@it ;
                                                                                        rdfs:label "Good Alignment"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Hero
<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Hero> rdf:type owl:Class ;
                                                                               rdfs:subClassOf :CreatureByRole ;
                                                                               owl:disjointWith :MonsterMovies ,
                                                                                                :Origin ,
                                                                                                :Power ;
                                                                               rdfs:comment "Creatura combattente contro un antagonista"@it ;
                                                                               rdfs:label "Hero"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#NeutralAlignment
<http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#NeutralAlignment> rdf:type owl:Class ;
                                                                                           rdfs:subClassOf <http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1#Alignment> ;
                                                                                           rdfs:comment "Creatura tendenzialmente buona ma con comportamenti disallineati a quelli di un eroe"@it ;
                                                                                           rdfs:label "Neutral Alignment"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/AggressiveCreature
:AggressiveCreature rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :participatesIn ;
                                          owl:someValuesFrom :Battle
                                        ] ;
                    rdfs:subClassOf :CreatureByRole ;
                    owl:disjointWith :MonsterMovies ,
                                     :Origin ,
                                     :Power ;
                    rdfs:comment "Una creatura che ha partecipato ad almeno una battaglia con un'altra, viene definita aggressiva"@it ;
                    rdfs:label "Aggressive Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Battle
:Battle rdf:type owl:Class ;
        owl:disjointWith :Franchise ,
                         :MonsterMovies ,
                         :Origin ,
                         :Power ;
        rdfs:comment "Evento per gestire le battaglie tra due creature"@it ;
        rdfs:label "Battle"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Creature
:Creature rdf:type owl:Class ;
          rdfs:subClassOf <http://dbpedia.org/ontology/FictionalCharacter> ;
          owl:disjointWith :MonsterMovies ,
                           :Origin ,
                           :Power ;
          rdfs:comment "Rappresenta un mostro o creatura cinematografica che appare in uno o più film"@it ;
          rdfs:label "Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByOrigin
:CreatureByOrigin rdf:type owl:Class ;
                  rdfs:subClassOf :Creature ;
                  owl:disjointWith :MonsterMovies ,
                                   :Origin ,
                                   :Power ;
                  rdfs:comment "Classificazione delle creature in base alla loro origine"@it ;
                  rdfs:label "Creature By Origin"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/CreatureByRole
:CreatureByRole rdf:type owl:Class ;
                rdfs:subClassOf :Creature ;
                owl:disjointWith :MonsterMovies ,
                                 :Origin ,
                                 :Power ;
                rdfs:comment "Categorizza le creature in base alla loro natura"@it ;
                rdfs:label "Creature By Role"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Crew
:Crew rdf:type owl:Class ;
      rdfs:subClassOf <http://schema.org/Person> ;
      rdfs:comment "Persone che hanno lavorato ad un Film"@it ;
      rdfs:label "Film Crew"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Director
:Director rdf:type owl:Class ;
          rdfs:subClassOf :Crew ;
          rdfs:comment "Regista cinematografico che ha diretto uno o più film"@it ;
          rdfs:label "Director"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/DominantCreature
:DominantCreature rdf:type owl:Class ;
                  rdfs:subClassOf :CreatureByRole ;
                  owl:disjointWith :MonsterMovies ,
                                   :Origin ,
                                   :Power ;
                  rdfs:comment "Creatura che vince più battaglie"@it ;
                  rdfs:label "Dominant Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Franchise
:Franchise rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :appearsIn ;
                                 owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                 owl:onClass :MonsterMovies
                               ] ;
           rdfs:subClassOf :Creature ;
           owl:disjointWith :MonsterMovies ,
                            :Origin ,
                            :Power ;
           rdfs:comment "Serie di film collegati tra loro, definita come un insieme di almeno due film correlati"@it ;
           rdfs:label "Franchise"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/HighlyDangerousCreature
:HighlyDangerousCreature rdf:type owl:Class ;
                         rdfs:subClassOf :CreatureByRole ;
                         owl:disjointWith :MonsterMovies ,
                                          :Origin ,
                                          :Power ;
                         rdfs:comment "Una creatura con un power level eccessivo"@it ;
                         rdfs:label "Highly Dangerous Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Loser
:Loser rdf:type owl:Class ;
       rdfs:subClassOf :CreatureByRole ;
       owl:disjointWith :MonsterMovies ,
                        :Origin ,
                        :Power ;
       rdfs:comment "Sconfitto in una battaglia di un film, la creatura che perde"@it ;
       rdfs:label "Loser"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/MonsterMovies
:MonsterMovies rdf:type owl:Class ;
               rdfs:subClassOf <http://schema.org/Movie> ;
               owl:disjointWith :NaturalCreature ,
                                :NuclearCreature ,
                                :Origin ,
                                :Power ,
                                :VampireCreature ;
               rdfs:comment "Pellicola cinematografica con una creatura di finzione all'interno"@it ;
               rdfs:label "Monster Movies"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/NaturalCreature
:NaturalCreature rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasOrigin ;
                                       owl:hasValue :Evolution
                                     ] ;
                 rdfs:subClassOf :CreatureByOrigin ;
                 owl:disjointWith :Origin ,
                                  :Power ;
                 rdfs:comment "Creatura generata per cause naturali"@it ;
                 rdfs:label "Natural Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/NuclearCreature
:NuclearCreature rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasOrigin ;
                                       owl:hasValue :Nuclear
                                     ] ;
                 rdfs:subClassOf :CreatureByOrigin ;
                 owl:disjointWith :Origin ,
                                  :Power ;
                 rdfs:comment "Creatura la cui origine è legata all'energia nucleare o alle radiazioni"@it ;
                 rdfs:label "Nuclear Creature"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Origin
:Origin rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Bite
                                          :Evolution
                                          :Infection
                                          :Nuclear
                                        )
                            ] ;
        owl:disjointWith :Power ,
                         :VampireCreature ;
        rdfs:comment "Classe enumerata che rappresenta le possibili origini di una creatura: morso, infezione o nucleare"@it ;
        rdfs:label "Origin"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/Power
:Power rdf:type owl:Class ;
       owl:disjointWith :VampireCreature ;
       rdfs:comment "Potere o abilità speciale posseduta da una creatura"@it ;
       rdfs:label "Power"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/VampireCreature
:VampireCreature rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasOrigin ;
                                       owl:hasValue :Bite
                                     ] ;
                 rdfs:subClassOf :CreatureByOrigin ;
                 rdfs:comment "Creatura la cui origine è legata al morso di un vampiro"@en ;
                 rdfs:label "Vampire Creature"@en .


