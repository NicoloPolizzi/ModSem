###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/firstMovie
:firstMovie rdf:type owl:DatatypeProperty ;
            rdfs:domain :Creature ;
            rdfs:range xsd:string ;
            rdfs:comment "Primo film in cui quella creatura appare"@it ;
            rdfs:label "firstMovie"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/height
:height rdf:type owl:DatatypeProperty ;
        rdfs:domain :Creature ;
        rdfs:range xsd:float ;
        rdfs:comment "Altezza della creatura"@it ;
        rdfs:label "height"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/powerLevel
:powerLevel rdf:type owl:DatatypeProperty ;
            rdfs:domain :Creature ;
            rdfs:range xsd:integer ;
            rdfs:comment "Livello numerico che indica la potenza di una creatura"@it ;
            rdfs:label "powerLevel"@en .


###  http://www.semanticweb.org/utente/ontologies/2025/9/Monster_Ontology_V1/releaseYear
:releaseYear rdf:type owl:DatatypeProperty ;
             rdfs:domain :MonsterMovies ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "Anno di uscita di un film"@it ;
             rdfs:label "releaseDate"@en .


