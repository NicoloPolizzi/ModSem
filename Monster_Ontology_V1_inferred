:c1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:c2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:a1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:a2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:b rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:c rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:w rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:m1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:m2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:b1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:b2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:f1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:f2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:lvl rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:o rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Se due creature hanno la stessa origine, sono creature affini" ;
   rdfs:label "SameOrigin" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :hasOrigin ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :c1 ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :o
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#propertyPredicate> :hasOrigin ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> :c2 ;
                                                                  <http://www.w3.org/2003/11/swrl#argument2> :o
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DifferentIndividualsAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> :c1 ;
                                                                             <http://www.w3.org/2003/11/swrl#argument2> :c2
                                                                           ] ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :sharesOriginWith ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :c1 ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :c2
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Creatura che combatte contro un Antagonist" ;
